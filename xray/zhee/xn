function dbzip2(){
clear

putih="\e[1;97m"
BGX="\e[104m"
blue="\e[1;96m"
NC='\e[0m'
clear
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
echo -e "${putih} ${BGX}             Tools Decrypt Bzip2               ${NC}"
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
echo -e " ${putih} File Name ( Nama file yang mau di Decrypt )${NC}"
echo -e " ${putih} Output File ( Hasil file yang di Decrypt ) ${NC}"
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
echo -e " "
read -p "   File Name : " name
read -p "   Output File : " path
sleep 0.5
echo -e "   sedang dalam proses "
sleep 0.5
echo -e "   proses sudah selesai "
sleep 0.5
tail -n +23 ${name} | bzip2 -cd >> ${path}
echo ""
read -n 1 -s -r -p "Press any key to back on menu"
dec
}
function shco(){
clear

putih="\e[1;97m"
BGX="\e[104m"
blue="\e[1;96m"
NC='\e[0m'
clear
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
echo -e "${putih} ${BGX}             Tools Decrypt SHC               ${NC}"
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
echo -e " ${putih} Contoh Sleep : 0.000 - 0.500 ( Sesuaikan yg Cocok ${NC}"
echo -e " ${putih} File Name ( Nama file yang mau di Decrypt )${NC}"
echo -e " ${putih} Hasil Decrypt Cek di ( ls ) nama file ( core )${NC}"
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"

ulimit -c unlimited
#read -p "   Perizinan File : " izin
read -p "   File Name : " name
read -p "   Sleep Time : " waktu
sleep 0.5
echo -e "   sedang dalam proses "
sleep 0.5
echo -e "   proses sudah selesai "
sleep 0.5
chmod +x ${name}
./${name} & ( sleep ${waktu} && kill -SIGSEGV $! )
echo ""
read -n 1 -s -r -p "Press any key to back on menu"
dec
}
function bash64(){
clear

putih="\e[1;97m"
BGX="\e[104m"
blue="\e[1;96m"
NC='\e[0m'
cd
clear
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
echo -e "${putih} ${BGX}               TOOLS DECRYPT BASE64               ${NC}"
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
echo -e " ${putih} File Name : Nama file yang mau di dec${NC}"
echo -e " ${putih} Out Name : nama file hasil decrypt${NC}"
echo -e "${blue}━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━${NC}"
read -p "File-Name : " file
read -p "Out-Name : " out
sleep 0.5
echo -e "sedang dalam proses "
sleep 0.5
echo -e "proses sudah selesai "
sleep 0.5
cat ${file} | tr ';' '\n' | grep 'RzE=' | cut -d '"' -f2 | tr ' ' '\n' | rev | base64 -d >> ${out}
echo -e ""
read -n 1 -s -r -p "Press any key to back on menu"
dec
}
