function bashh(){
clear
YELLOW=$(tput setaf 3)
GREEN=$(tput setaf 2)
RED=$(tput setaf 1)
BLUE=$(tput setaf 4)
STAND=$(tput sgr 0)
BOLD=$(tput bold)
pwd=$(pwd)
echo "
  ${GREEN}

╔══╗──────╔╗──╔═══╗─────╔╗
║╔╗║──────║║──║╔═╗║─────║║
║╚╝╚╦══╦══╣╚═╗║╚═╝╠══╦══╣║╔╗
║╔═╗║╔╗║══╣╔╗║║╔╗╔╣╔╗║╔═╣╚╝╝
║╚═╝║╔╗╠══║║║║║║║╚╣╚╝║╚═╣╔╗╗
╚═══╩╝╚╩══╩╝╚╝╚╝╚═╩══╩══╩╝╚╝" 
echo -e ""
echo -e " Encrypt your Bash Shell/Script"
echo -e "$BOLD\n input file  : \c"
read infile

ls | grep $infile >> /dev/null
if [ "$?" != 0 ];then
    echo ""
    echo " File Nof Found"
    exit 1
fi
echo -e " output file : \c"
read outfile

u=`cat $infile | base64 | rev`

